Main GUI Table:
---------------
CREATE TABLE gui_main_data (id int(11) NOT NULL AUTO_INCREMENT, server_ip varchar(15) DEFAULT NULL, amber_color varchar(10) DEFAULT NULL, server_type varchar(100) DEFAULT NULL, monitor_time datetime DEFAULT NULL, PRIMARY KEY (`id`));

Server data Tables:
--------------------
CREATE TABLE disk_space_stats (id INT NOT NULL AUTO_INCREMENT, server_ip VARCHAR(15), drive_name VARCHAR(100), total_size VARCHAR(10), size_used VARCHAR(10), available_size VARCHAR(10), use_percent VARCHAR(10), monitor_time VARCHAR(15), data_insert_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, indicator_status VARCHAR(20), PRIMARY KEY(id));
CREATE TABLE free_memory_stats (id INT NOT NULL AUTO_INCREMENT, server_ip VARCHAR(15), memory_type VARCHAR(40), total_memory VARCHAR(5), used_memory VARCHAR(5), free_memory VARCHAR(5), shared_memory VARCHAR(5), buffers VARCHAR(5), cached VARCHAR(5), monitor_time VARCHAR(15), data_insert_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, indicator_status VARCHAR(20), PRIMARY KEY(id));
CREATE TABLE load_average_stats (id INT NOT NULL AUTO_INCREMENT, server_ip VARCHAR(15), no_of_users VARCHAR(10), uptime VARCHAR(150), one_minute_average VARCHAR(100), five_minute_average VARCHAR(100), fifteen_minute_average VARCHAR(100), monitor_time VARCHAR(20), data_insert_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, indicator_status VARCHAR(20), PRIMARY KEY(id));
CREATE TABLE logged_users_stats (id INT NOT NULL AUTO_INCREMENT, server_ip VARCHAR(15), username VARCHAR(100), terminal VARCHAR(100), logged_in_from VARCHAR(100), login_time VARCHAR(50), idle_time VARCHAR(50), monitor_time VARCHAR(20), data_insert_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, indicator_status VARCHAR(20), PRIMARY KEY(id));
CREATE TABLE java_process_stats (id INT NOT NULL AUTO_INCREMENT, server_ip VARCHAR(15), username VARCHAR(100), process_id VARCHAR(20), parent_process_id VARCHAR(20), processor_utilization VARCHAR(40), start_time VARCHAR(30), process_address TEXT, monitor_time VARCHAR(20), data_insert_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, indicator_status VARCHAR(20), PRIMARY KEY(id));
CREATE TABLE mysql_process_stats (id INT NOT NULL AUTO_INCREMENT, server_ip VARCHAR(15), username VARCHAR(100), process_id VARCHAR(20), parent_process_id VARCHAR(20), processor_utilization VARCHAR(40), start_time VARCHAR(30), process_address TEXT, monitor_time VARCHAR(20), data_insert_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, indicator_status VARCHAR(20), PRIMARY KEY(id));
CREATE TABLE O3_process_stats (id INT NOT NULL AUTO_INCREMENT, server_ip VARCHAR(15), username VARCHAR(100), process_id VARCHAR(20), parent_process_id VARCHAR(20), processor_utilization VARCHAR(40), start_time VARCHAR(30), process_address TEXT, monitor_time VARCHAR(20), data_insert_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP, indicator_status VARCHAR(20), PRIMARY KEY(id));
